@{
    ViewBag.Title = "表字段管理";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}

<div id="layout">
    <div region="west" iconCls="icon16_table" split="true" title="数据表" style="width:280px;padding: 5px" collapsible="false">
        <ul id="tbData"></ul>
    </div>
    <div region="center" title="表字段明细" iconCls="icon16_column_double" style="padding: 2px; overflow: hidden">
